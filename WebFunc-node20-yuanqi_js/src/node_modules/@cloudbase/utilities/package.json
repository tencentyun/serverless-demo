{
  "name": "@cloudbase/utilities",
  "version": "2.25.4",
  "description": "cloudbase javascript sdk utilities",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "miniprogram": "dist/miniprogram",
  "scripts": {
    "tsc": "rm -rf dist/ && tsc -p tsconfig.json && tsc -p tsconfig.esm.json",
    "lint": "eslint --fix \"./src/**/*.ts\"",
    "build": "npm run lint&& npm run tsc && npm run build:mp",
    "build:mp": "webpack --config ./webpack/webpack.miniprogram.js",
    "precommit": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TencentCloudBase/cloudbase-js-sdk"
  },
  "keywords": [
    "tcb",
    "cloudbase",
    "serverless"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@cloudbase/adapter-interface": "^0.7.1",
    "@cloudbase/types": "2.25.4",
    "jwt-decode": "^3.1.2"
  },
  "devDependencies": {
    "@types/node": "^22.5.4"
  },
  "gitHead": "cb3b43f79a34d6216a45f065ab940c4225fe55d5"
}
