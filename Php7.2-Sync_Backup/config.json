            "version": "1.3.2"  // Version number, which identifies the demo version status. // Demo will not to update to the page if the version number unmodified.//版本号，通过版本号标识 demo 升级情况，未修改版本号会导致 demo 不更新至页面
        },
        "functions": {
            "name": "COS_Sync_Backup",// Function name which only be in English. // 函数名称，只支持英文
            "description": "This demo will backup the COS bucket object to another bucket when you upload the file. 此示例使用COS作为触发器，在用户上传文件到指定bucket时，将该文件复制到另一个用于备份的bucket存储",
            "handler": "sync_backup.main_handler",
            "memorySize": 128,// Running memory. 运行配置内存
            "timeout": 10,// Running timeout. 运行超时时间
            "runtime": "Php7",//Runtime which users can search demo by this keyword. 运行环境，用户可以通过该关键字搜索["Python2.7", "Python3.6", "Nodejs6.10", "Java8", "LuaCDN", "NodejsCDN", "Php5", "Php7", "Nodejs8.9", "Go1"]
            "Environment": {},// Optional, used to define environment variables. 可选，用于定义环境变量
            "Events": {
                "Type": "COS",
                "Properties": {
                    "Bucket": "mycosbucket",
                    "Events": "cos:ObjectCreated:*",
                    "Filter": "",
                    "Enable": "true"
                }
            },
            // Optional, used define the event source that triggers this function.  可选，用于定义触发此函数的事件源
            "VpcConfig": {},
            // Optional, used configure cloud function's private network. 可选， 用于配置云函数访问 VPC 私有网络。
            "codeObject": {
                "codeFile": [
                    "sync_backup.php"
                ],
                "CodeUri": [
                    // Code download url which should be same as 'download_address'. 代码下载地址，和download_address保持一致
                    "https://github.com/tencentyun/scf-demo-repo/tree/master/Php7-Sync_Backup"
                ]
            }
        }
